using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSlight
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int lastNumber = 100;

            for (int firstNumber = 5; firstNumber < lastNumber; firstNumber += 7)
            {
                Console.WriteLine(firstNumber);
            }
        }

    }
}
